# MentalSpace EHR - Environment Configuration Template
# Copy this file to .env and fill in your actual values
# NEVER commit .env to version control

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_PUBLISHABLE_KEY=your-publishable-anon-key-here
VITE_SUPABASE_PROJECT_ID=your-project-id

# Application Configuration
VITE_APP_NAME=MentalSpace EHR
VITE_APP_URL=https://your-domain.com

# Environment (development, staging, production)
VITE_ENV=development

# Feature Flags
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_TELEHEALTH=true
VITE_ENABLE_PORTAL=true

# Monitoring (Optional - for production)
# VITE_SENTRY_DSN=
# VITE_ANALYTICS_ID=

# API Keys (Optional)
# VITE_OPENAI_API_KEY=

# Twilio Video Configuration (Required for Telehealth)
# Get these from https://console.twilio.com
# TWILIO_ACCOUNT_SID=your-account-sid
# TWILIO_API_KEY=your-api-key
# TWILIO_API_SECRET=your-api-secret

# AdvancedMD API Configuration (Required for Billing Integration)
# Get credentials from AdvancedMD support team

# Environment: sandbox or production
VITE_ADVANCEDMD_ENVIRONMENT=sandbox

# Sandbox Configuration
VITE_ADVANCEDMD_SANDBOX_BASE_URL=https://api-sandbox.advancedmd.com/v1
VITE_ADVANCEDMD_SANDBOX_OFFICE_KEY=your-sandbox-office-key
VITE_ADVANCEDMD_SANDBOX_API_USERNAME=your-sandbox-username
VITE_ADVANCEDMD_SANDBOX_API_PASSWORD=your-sandbox-password
VITE_ADVANCEDMD_SANDBOX_CLIENT_ID=your-sandbox-client-id
VITE_ADVANCEDMD_SANDBOX_CLIENT_SECRET=your-sandbox-client-secret

# Production Configuration (Keep these secure!)
VITE_ADVANCEDMD_PROD_BASE_URL=https://api.advancedmd.com/v1
VITE_ADVANCEDMD_PROD_OFFICE_KEY=your-production-office-key
VITE_ADVANCEDMD_PROD_API_USERNAME=your-production-username
VITE_ADVANCEDMD_PROD_API_PASSWORD=your-production-password
VITE_ADVANCEDMD_PROD_CLIENT_ID=your-production-client-id
VITE_ADVANCEDMD_PROD_CLIENT_SECRET=your-production-client-secret
